{
  "name": "RBXIDLE",
  "version": "2.5.1",
  "description": "A robux mining application",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --x64 --ia32",
    "gh-publish": "electron-builder --x64 --ia32 -p always",
    "build": "electron-builder build --win --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "author": "Tox",
  "license": "UNLICENSED",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^15.1.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "git": "^0.1.5"
  },
  "build": {
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/dev-app-update.yml",
      "!**/resources/vars/*",
      "!**temp.js"
    ],
    "appId": "com.rbxidle.app",
    "publish": [
      {
        "provider": "github",
        "owner": "tox-lol",
        "repo": "rbxidle"
      }
    ],
    "extraResources": [
      "./components/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tox-lol/rbxidle.git"
  },
  "dependencies": {
    "bytenode": "^1.3.3",
    "cryptr": "^6.0.2",
    "custom-electron-titlebar": "^3.2.10",
    "decompress-zip": "^0.3.3",
    "discord-rpc": "^4.0.1",
    "electron-dl": "^3.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-sudo": "^4.0.12",
    "electron-updater": "^4.6.1",
    "node-windows": "^1.0.0-beta.7",
    "request": "^2.88.2",
    "semver": "^7.3.5",
    "sudo-prompt": "^9.2.1",
    "xml2js": "^0.4.23"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rbxidle_elec"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
